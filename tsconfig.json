{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "declaration": false,
    "paths": {
      "@/*": ["app/*"]
    }
  },
  "exclude": ["node_modules", ".sst", ".output"]
}
